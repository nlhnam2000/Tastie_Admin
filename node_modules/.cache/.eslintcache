 primaryDark, secondaryMain, secondaryLight, primary, darkLight, grey200, isLoading, grey500, data, type]);\r\n\r\n    return (\r\n        <>\r\n            {isLoading ? (\r\n                <SkeletonTotalGrowthBarChart />\r\n            ) : (\r\n                <MainCard>\r\n                    <Grid container spacing={gridSpacing}>\r\n                        <Grid item xs={12}>\r\n                            <Grid container alignItems=\"center\" justifyContent=\"space-between\">\r\n                                <Grid item>\r\n                                    <Grid container direction=\"column\" spacing={1}>\r\n                                        <Grid item>\r\n                                            {(() => {\r\n                                                switch (type) {\r\n                                                    case 1:\r\n                                                        return <Typography variant=\"subtitle1\">Total revenue</Typography>;\r\n                                                    /* falls through */\r\n\r\n                                                    case 2:\r\n                                                        return <Typography variant=\"subtitle1\">Total Sale</Typography>;\r\n                                                    /* falls through */\r\n\r\n                                                    case 3:\r\n                                                        return <Typography variant=\"subtitle1\">Sale per order</Typography>;\r\n                                                    /* falls through */\r\n\r\n                                                    default:\r\n                                                        return <Typography variant=\"subtitle1\">Total revenue</Typography>;\r\n                                                    /* falls through */\r\n                                                }\r\n                                            })()}\r\n               